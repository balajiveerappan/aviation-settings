server:
  port: 8087
spring:
  jpa:
    properties:
      hibernate.dialect:
        org.hibernate.dialect.MySQL5Dialect
---
spring:
  profiles:
    active: cloud
  datasource:
    url: jdbc:mysql://localhost:3306/aviation?useSSL=false&user=root&password=root
eureka:
  instance:
    non-secure-port: ${CF_INSTANCE_PORT:${PORT:${server.port:8761}}}
    hostname: ${CF_INSTANCE_IP:localhost}
    secure-virtual-host-name: ${spring.application.name}

  client:
    service-url:
      defaultZone: ${vcap.services.eureka.credentials.url}
---  
spring:
  profiles:
    active: local
  datasource:
    url: jdbc:mysql://localhost:3306/aviation?useSSL=false&user=root&password=root
    